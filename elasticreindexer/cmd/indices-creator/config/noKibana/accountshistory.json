{
 "index_patterns": [
  "accountshistory-*"
 ],
 "mappings": {
  "properties": {
   "address": {
    "type": "keyword"
   },
   "balance": {
    "type": "keyword"
   },
   "isSender": {
    "type": "boolean"
   },
   "isSmartContract": {
    "type": "boolean"
   },
   "shardID": {
    "type": "long"
   },
   "timestamp": {
    "format": "epoch_second",
    "type": "date"
   }
  }
 },
 "settings": {
  "number_of_replicas": 0,
  "number_of_shards": 5
 }
}
